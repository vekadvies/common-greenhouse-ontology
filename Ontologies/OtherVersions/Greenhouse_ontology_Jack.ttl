    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasB_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasCa_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasCl_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasCu_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasEC ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasFe_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasHCO3_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasK_Ca_ratio ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasK_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasMg_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasMn_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasMo_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasNH4_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasNO3_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasNa_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasP_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasS_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasSi_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasZn_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:haspH ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasN_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasN_total_level ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasSugar_concentration ;
    ] ;
.
ghm:PlantGrowth
  rdf:type owl:Class ;
  rdfs:comment "This class represents growth measurements of a single plant" ;
  rdfs:label "plant growth" ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasLength ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasNumberOfLeavesPerStem ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasPlantNumber ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasStemThickness ;
    ] ;
.
ghm:PlantGrowthMeasurement
  rdf:type owl:Class ;
  rdfs:comment "This represents a measurement of the individual growth of a set of plants as well as some general parameters over the entire set, such as average length or color. " ;
  rdfs:label "Plant growth measurement" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasAverageLength ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasAverageNumberOfLeavesPerStem ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasAverageStemThickness ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasLeafColor ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasRootColor ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasRootDevelopment ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasUniformity ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass ghm:Block ;
      owl:onProperty ghm:isMeasuredInBlock ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:hasPlantGrowth ;
      owl:someValuesFrom ghm:PlantGrowth ;
    ] ;
.
ghm:System
  rdf:type owl:Class ;
  rdfs:label "System" ;
  rdfs:subClassOf owl:Thing ;
.
ghm:Trait
  rdf:type owl:Class ;
  rdfs:comment "A trait is a characteristic of an individual plant or a group of plants that can be observed and that results from the expression of its genes and their interaction with the environment." ;
  rdfs:comment "Examples of a trait are: fruit shape, seed color, stem diameter, seed length, ...." ;
  rdfs:label "Trait" ;
  rdfs:subClassOf owl:Thing ;
.
ghm:Valve
  rdf:type owl:Class ;
  rdfs:label "Valve" ;
  rdfs:subClassOf owl:Thing ;
.
ghm:WaterAnalysis
  rdf:type owl:Class ;
  rdfs:comment "This class represents analysis on nutritions in a sample of water that is used to irrigate plants in a greenhouse" ;
  rdfs:label "Water analysis" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasNutritionRecipe ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasSampleName ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasWeekDay ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasWeekNumber ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass ghm:NutritionLevel ;
      owl:onProperty ghm:hasNutritionLevel ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
ghm:WaterStorageSystem
  rdf:type owl:Class ;
  rdfs:label "Water storage system" ;
  rdfs:subClassOf ghm:WaterSystem ;
.
ghm:WaterSystem
  rdf:type owl:Class ;
  rdfs:comment "This class represents the systems in the greenhouse that deal with water. This includes the infrastructure of the systems as well as the management functionality." ;
  rdfs:label "Water system" ;
  rdfs:subClassOf ghm:System ;
.
ghm:Zone
  rdf:type owl:Class ;
  rdfs:comment "A zone is an area in which the same irrigation and fertilization strategy is applied" ;
  rdfs:label "Zone" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:contains ;
      owl:someValuesFrom ghm:IrrigationValve ;
    ] ;
.
ghm:builds
  rdf:type owl:ObjectProperty ;
  rdfs:domain ghm:Builder ;
  rdfs:label "builds" ;
  rdfs:range ghm:Greenhouse ;
  owl:inverseOf ghm:isBuildBy ;
.
ghm:contains
  rdf:type owl:ObjectProperty ;
  rdfs:label "contains" ;
.
ghm:containsBlocks
  rdf:type owl:ObjectProperty ;
  rdfs:domain ghm:Greenhouse ;
  rdfs:label "contains blocks" ;
  rdfs:range ghm:Block ;
  rdfs:subPropertyOf ghm:contains ;
.
ghm:grows
  rdf:type owl:ObjectProperty ;
  rdfs:domain ghm:Grower ;
  rdfs:label "grows" ;
  rdfs:range ghm:Crop ;
  owl:inverseOf ghm:isGrownBy ;
.
ghm:hasAl_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of aluminium in a sample of water measured in µmol/l" ;
  rdfs:label "has al level" ;
  rdfs:range xsd:float ;
.
ghm:hasAverageLength
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The average length of all the plants in a measurement in centimeters" ;
  rdfs:domain ghm:PlantGrowthMeasurement ;
  rdfs:label "Average length" ;
  rdfs:range xsd:integer ;
.
ghm:hasAverageNumberOfLeavesPerStem
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The average number of leaves per stem of all the plants in a measurement" ;
  rdfs:domain ghm:PlantGrowthMeasurement ;
  rdfs:label "has average number of leaves per stem" ;
  rdfs:range xsd:integer ;
.
ghm:hasAverageStemThickness
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The average thickness of the stem of all plants in a measurement in centimeters with 2 decimals measured 5 centimeters below the knot of the plant" ;
  rdfs:domain ghm:PlantGrowthMeasurement ;
  rdfs:label "has average stem thickness" ;
  rdfs:range xsd:float ;
.
ghm:hasB_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of boron in a sample of water measured in µmol/l" ;
  rdfs:label "has B level" ;
  rdfs:range xsd:float ;
.
ghm:hasBlockIdentifier
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "Each block has a unique identifier in the greenhouse or compartment in the greenhouse" ;
  rdfs:label "has block identifier" ;
  rdfs:range xsd:string ;
.
ghm:hasCa_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of calcium in a sample of water measured in mmol/l" ;
  rdfs:label "has ca level" ;
  rdfs:range xsd:float ;
.
ghm:hasCl_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of chloride in a sample of water measured in mmol/l" ;
  rdfs:label "has cl level" ;
  rdfs:range xsd:float ;
.
ghm:hasCu_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of copper in a sample of water measured in µmol/l" ;
  rdfs:label "has cu level" ;
  rdfs:range xsd:float ;
.
ghm:hasDate
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The date at which a measurement in the greenhouse has been executed, e.g. on the growth of a plant, the quality of the water etc." ;
  rdfs:label "has date" ;
  rdfs:range xsd:date ;
.
ghm:hasEC
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the electrical conductivity of a sample of water measured in milliSiemens per centimeter (mS/cm)" ;
  rdfs:label "has EC" ;
  rdfs:range xsd:float ;
.
ghm:hasFe_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of iron in a sample of water measured in µmol/l" ;
  rdfs:label "has fe level" ;
  rdfs:range xsd:float ;
.
ghm:hasFlowerConstruction
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The preception of the flower construction of a plant as a number between 1 and 10 from bad to good" ;
  rdfs:domain ghm:PlantGrowthMeasurement ;
  rdfs:label "has flower construction" ;
  rdfs:range xsd:integer ;
.
ghm:hasHCO3_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of bicarbonate in a sample of water measured in mmol/l" ;
  rdfs:label "has HCO3 level" ;
