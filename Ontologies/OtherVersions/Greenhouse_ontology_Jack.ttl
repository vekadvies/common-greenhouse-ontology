###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasNutritionRecipe
ghm:hasNutritionRecipe rdf:type owl:DatatypeProperty ;
                       rdfs:range xsd:string ;
                       rdfs:comment "This is the name of the nutrition recipe that is used to irrigate plants in a greenhouse" ;
                       rdfs:label "has nutrition recipe" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasP_level
ghm:hasP_level rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:float ;
               rdfs:comment "This is the level of phosphorus in a sample of water measured in mmol/l" ;
               rdfs:label "has P level" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasPlantNumber
ghm:hasPlantNumber rdf:type owl:DatatypeProperty ;
                   rdfs:domain ghm:PlantGrowth ;
                   rdfs:range xsd:integer ;
                   rdfs:comment "The number of the plant as part of a measurement performed on its growth" ;
                   rdfs:label "has plant number" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasRootColor
ghm:hasRootColor rdf:type owl:DatatypeProperty ;
