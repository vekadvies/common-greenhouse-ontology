.
ghm:hasK_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of kalium in a sample of water measured in mmol/l" ;
  rdfs:label "has k level" ;
  rdfs:range xsd:float ;
.
ghm:hasLeafColor
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The preception of the greenness of the leafs of a plant as a number between 1 and 10 from yellow to dark green" ;
  rdfs:domain ghm:PlantGrowthMeasurement ;
  rdfs:label "has leaf color" ;
  rdfs:range xsd:integer ;
.
ghm:hasLength
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The length of a plant in centimeters" ;
  rdfs:domain ghm:PlantGrowth ;
  rdfs:label "Length" ;
  rdfs:range xsd:integer ;
.
ghm:hasMg_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of magnesium in a sample of water measured in mmol/l" ;
  rdfs:label "has mg level" ;
  rdfs:range xsd:float ;
.
ghm:hasMn_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of manganese in a sample of water measured in µmol/l" ;
  rdfs:label "has k level" ;
  rdfs:range xsd:float ;
.
ghm:hasMo_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of molybdenum in a sample of water measured in µmol/l" ;
  rdfs:label "has mo level" ;
  rdfs:range xsd:float ;
.
ghm:hasNH4_level
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the level of ammonium in a sample of water measured in mmol/l" ;
  rdfs:label "has NH4 level" ;
  rdfs:range xsd:float ;
.
