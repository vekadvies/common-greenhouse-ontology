# baseURI: https://www.tno.nl/agrifood/ontology/greenhouse-model/
# imports: https://raw.githubusercontent.com/HajoRijgersberg/OM/master/om-2.0.rdf
# prefix: ghm

@prefix MathM: <http://www.w3.org/1998/Math/MathML> .
@prefix ghm: <https://www.tno.nl/agrifood/ontology/greenhouse-model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix um: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

owl:Thing
  rdf:type owl:Class ;
.
ghm:
  rdf:type owl:Ontology ;
  spin:imports <http://topbraid.org/spin/owlrl-all> ;
  owl:imports <https://raw.githubusercontent.com/HajoRijgersberg/OM/master/om-2.0.rdf> ;
.
ghm:Block
  rdf:type owl:Class ;
  rdfs:comment "A block contains only a single crop (this is a requirement)" ;
  rdfs:comment "A block is part of a compartment in the greenhouse in which the plants are treated equally wrt nutrition, climate etc." ;
  rdfs:comment "Each block has a unique identifier in the greenhouse or compartment in the greenhouse" ;
  rdfs:comment "This class represents blocks that are part of a greenhouse and contain a set of plants" ;
  rdfs:label "Block" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasBlockIdentifier ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass ghm:Crop ;
      owl:onProperty ghm:contains ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
ghm:Builder
  rdf:type owl:Class ;
  rdfs:comment "This class represents builders that build greenhouses and possibly support growers in their crop management activities" ;
  rdfs:label "Builder" ;
  rdfs:subClassOf owl:Thing ;
.
ghm:Compartment
  rdf:type owl:Class ;
  rdfs:comment "A compartment is a part of the greenhouse" ;
  rdfs:comment "Each compartment has a unique identifier in the greenhouse" ;
  rdfs:comment "This class represents compartments that are part of a greenhouse and contain a set of plants" ;
  rdfs:label "Block" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:contains ;
      owl:someValuesFrom ghm:Block ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:contains ;
      owl:someValuesFrom ghm:Zone ;
    ] ;
.
ghm:Crop
  rdf:type owl:Class ;
  rdfs:comment "More characteristics of a crop can be defined using the guideline for creating a crop ontology published here: http://www.cropontology.org" ;
  rdfs:comment "This class represents crops that are produced in a greenhouse operated by a grower" ;
  rdfs:label "Crop" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasName ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:isGrownBy ;
      owl:someValuesFrom ghm:Grower ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:isGrownIn ;
      owl:someValuesFrom ghm:Greenhouse ;
    ] ;
.
ghm:DisinfectionSystem
  rdf:type owl:Class ;
  rdfs:label "Disinfection system" ;
  rdfs:subClassOf ghm:WaterSystem ;
.
ghm:Greenhouse
  rdf:type owl:Class ;
  rdfs:comment "This class represents greenhouses in which crop is being grown by a grower" ;
  rdfs:label "Greenhouse" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass ghm:Builder ;
      owl:onProperty ghm:isBuildBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass ghm:Grower ;
      owl:onProperty ghm:isOperatedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:contains ;
      owl:someValuesFrom ghm:Compartment ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:contains ;
      owl:someValuesFrom ghm:System ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:containsBlocks ;
      owl:someValuesFrom ghm:Block ;
    ] ;
.
ghm:Grower
  rdf:type owl:Class ;
  rdfs:comment "This class represents growers that operate and possibly own a greenhouse to produce one or more crops as efficient and effective as possible" ;
  rdfs:label "Grower" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:grows ;
      owl:someValuesFrom ghm:Crop ;
    ] ;
.
ghm:IrrigationValve
  rdf:type owl:Class ;
  rdfs:comment "An irrigation valve is a switch that steers the amount of water in a zone" ;
  rdfs:label "Irrigation valve" ;
  rdfs:subClassOf ghm:Valve ;
.
ghm:IrrigtationSystem
  rdf:type owl:Class ;
  rdfs:label "Irrigtation system" ;
  rdfs:subClassOf ghm:WaterSystem ;
.
ghm:LeafJuiceAnalysis
  rdf:type owl:Class ;
  rdfs:comment "This class represents analysis on nutritions in a sample of leaf juice of plants in a greenhouse" ;
  rdfs:label "Leaf juice analysis" ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasDate ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasNutritionRecipe ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasSampleName ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:hasWeekNumber ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ghm:ofCropPart ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onClass ghm:NutritionLevel ;
      owl:onProperty ghm:hasNutritionLevel ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty ghm:ofCrop ;
      owl:someValuesFrom ghm:Crop ;
    ] ;
.
