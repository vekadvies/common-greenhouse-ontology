@prefix : <https://www.tno.nl/agrifood/ontology/greenhouse-model/> .
@prefix um: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix ghm: <https://www.tno.nl/agrifood/ontology/greenhouse-model/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix MathM: <http://www.w3.org/1998/Math/MathML> .
@base <https://www.tno.nl/agrifood/ontology/greenhouse-model/> .

<https://www.tno.nl/agrifood/ontology/greenhouse-model/> rdf:type owl:Ontology ;
                                                          owl:imports <https://raw.githubusercontent.com/HajoRijgersberg/OM/master/om-2.0.rdf> ;
                                                          spin:imports <http://topbraid.org/spin/owlrl-all> .

#################################################################
#    Annotation properties
#################################################################

###  http://spinrdf.org/spin#imports
spin:imports rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gDay
xsd:gDay rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://www.tno.nl/agrifood/ontology/greenhouse-model/builds
ghm:builds rdf:type owl:ObjectProperty ;
           owl:inverseOf ghm:isBuildBy ;
           rdfs:domain ghm:Builder ;
           rdfs:range ghm:Greenhouse ;
           rdfs:label "builds" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/contains
ghm:contains rdf:type owl:ObjectProperty ;
             rdfs:label "contains" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/containsBlocks
ghm:containsBlocks rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf ghm:contains ;
                   rdfs:domain ghm:Greenhouse ;
                   rdfs:range ghm:Block ;
                   rdfs:label "contains blocks" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/grows
ghm:grows rdf:type owl:ObjectProperty ;
          owl:inverseOf ghm:isGrownBy ;
          rdfs:domain ghm:Grower ;
          rdfs:range ghm:Crop ;
          rdfs:label "grows" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasNutritionLevel
ghm:hasNutritionLevel rdf:type owl:ObjectProperty ;
                      rdfs:domain ghm:WaterAnalysis ;
                      rdfs:range ghm:NutritionLevel ;
                      rdfs:label "has nutrition level" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasPlantGrowth
ghm:hasPlantGrowth rdf:type owl:ObjectProperty ;
                   rdfs:domain ghm:PlantGrowthMeasurement ;
                   rdfs:range ghm:PlantGrowth ;
                   rdfs:label "has plant growth" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/isBuildBy
ghm:isBuildBy rdf:type owl:ObjectProperty ;
              rdfs:domain ghm:Greenhouse ;
              rdfs:range ghm:Builder ;
              rdfs:label "is build by" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/isGrownBy
ghm:isGrownBy rdf:type owl:ObjectProperty ;
              rdfs:domain ghm:Crop ;
              rdfs:range ghm:Grower ;
              rdfs:label "is grown by" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/isGrownIn
ghm:isGrownIn rdf:type owl:ObjectProperty ;
              rdfs:domain ghm:Crop ;
              rdfs:range ghm:Greenhouse ;
              rdfs:label "is grown in" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/isMeasuredInBlock
ghm:isMeasuredInBlock rdf:type owl:ObjectProperty ;
                      rdfs:domain ghm:PlantGrowthMeasurement ;
                      rdfs:range ghm:Block ;
                      rdfs:label "is located in block" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/isOperatedBy
ghm:isOperatedBy rdf:type owl:ObjectProperty ;
                 owl:inverseOf ghm:operates ;
                 rdfs:domain ghm:Greenhouse ;
                 rdfs:range ghm:Grower ;
                 rdfs:label "is operated by" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/ofCrop
ghm:ofCrop rdf:type owl:ObjectProperty ;
           rdfs:domain ghm:LeafJuiceAnalysis ;
           rdfs:range ghm:Crop ;
           rdfs:comment "This property represents the relation between an analysis and the crop that is analyzed" ;
           rdfs:label "of crop" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/operates
ghm:operates rdf:type owl:ObjectProperty ;
             rdfs:domain ghm:Grower ;
             rdfs:range ghm:Greenhouse ;
             rdfs:label "operates" .


#################################################################
#    Data properties
#################################################################

