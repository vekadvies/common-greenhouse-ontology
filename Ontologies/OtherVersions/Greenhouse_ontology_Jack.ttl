ghm:hasSi_level rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:float ;
                rdfs:comment "This is the level of silicon in a sample of water measured in mmol/l" ;
                rdfs:label "has si level" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasStemThickness
ghm:hasStemThickness rdf:type owl:DatatypeProperty ;
                     rdfs:domain ghm:PlantGrowth ;
                     rdfs:range xsd:float ;
                     rdfs:comment "The thickness of the stem in centimeters with 2 decimals measured 5 centimeters below the knot of the plant" ;
                     rdfs:label "has stem thickness" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasSugar_concentration
ghm:hasSugar_concentration rdf:type owl:DatatypeProperty ;
                           rdfs:range xsd:float ;
                           rdfs:comment "This is the concentration of sugar in a sample of a fluid measured in a percentage" ;
                           rdfs:label "has sugar concentration" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasUniformity
ghm:hasUniformity rdf:type owl:DatatypeProperty ;
                  rdfs:domain ghm:PlantGrowthMeasurement ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "The preception of the uniformity of a plant as a number between 1 and 10 from bad to good" ;
                  rdfs:label "has uniformity" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasWeekDay
ghm:hasWeekDay rdf:type owl:DatatypeProperty ;
