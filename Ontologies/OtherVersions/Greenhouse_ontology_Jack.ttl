                       rdfs:comment "Each block has a unique identifier in the greenhouse or compartment in the greenhouse" ;
                       rdfs:label "has block identifier" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasCa_level
ghm:hasCa_level rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:float ;
                rdfs:comment "This is the level of calcium in a sample of water measured in mmol/l" ;
                rdfs:label "has ca level" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasCl_level
ghm:hasCl_level rdf:type owl:DatatypeProperty ;
