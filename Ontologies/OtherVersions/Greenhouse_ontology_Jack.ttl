#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2002/07/owl#Thing
owl:Thing rdf:type owl:Class .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/Block
ghm:Block rdf:type owl:Class ;
          rdfs:subClassOf owl:Thing ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ghm:contains ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass ghm:Crop
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ghm:hasBlockIdentifier ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          rdfs:comment "A block contains only a single crop (this is a requirement)" ,
                       "A block is part of a compartment in the greenhouse in which the plants are treated equally wrt nutrition, climate etc." ,
                       "Each block has a unique identifier in the greenhouse or compartment in the greenhouse" ,
                       "This class represents blocks that are part of a greenhouse and contain a set of plants" ;
          rdfs:label "Block" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/Builder
ghm:Builder rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing ;
            rdfs:comment "This class represents builders that build greenhouses and possibly support growers in their crop management activities" ;
            rdfs:label "Builder" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/Compartment
ghm:Compartment rdf:type owl:Class ;
                rdfs:subClassOf owl:Thing ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty ghm:contains ;
                                  owl:someValuesFrom ghm:Block
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty ghm:contains ;
                                  owl:someValuesFrom ghm:Zone
                                ] ;
                rdfs:comment "A compartment is a part of the greenhouse" ,
                             "Each compartment has a unique identifier in the greenhouse" ,
                             "This class represents compartments that are part of a greenhouse and contain a set of plants" ;
                rdfs:label "Block" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/Crop
ghm:Crop rdf:type owl:Class ;
         rdfs:subClassOf owl:Thing ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ghm:isGrownBy ;
                           owl:someValuesFrom ghm:Grower
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ghm:isGrownIn ;
                           owl:someValuesFrom ghm:Greenhouse
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ghm:hasName ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ;
         rdfs:comment "More characteristics of a crop can be defined using the guideline for creating a crop ontology published here: http://www.cropontology.org" ,
                      "This class represents crops that are produced in a greenhouse operated by a grower" ;
         rdfs:label "Crop" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/DisinfectionSystem
ghm:DisinfectionSystem rdf:type owl:Class ;
                       rdfs:subClassOf ghm:WaterSystem ;
                       rdfs:label "Disinfection system" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/Greenhouse
ghm:Greenhouse rdf:type owl:Class ;
               rdfs:subClassOf owl:Thing ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty ghm:contains ;
                                 owl:someValuesFrom ghm:Compartment
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty ghm:contains ;
                                 owl:someValuesFrom ghm:System
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty ghm:containsBlocks ;
                                 owl:someValuesFrom ghm:Block
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty ghm:isBuildBy ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass ghm:Builder
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty ghm:isOperatedBy ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass ghm:Grower
                               ] ;
               rdfs:comment "This class represents greenhouses in which crop is being grown by a grower" ;
               rdfs:label "Greenhouse" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/Grower
ghm:Grower rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty ghm:grows ;
                             owl:someValuesFrom ghm:Crop
                           ] ;
           rdfs:comment "This class represents growers that operate and possibly own a greenhouse to produce one or more crops as efficient and effective as possible" ;
           rdfs:label "Grower" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/IrrigationValve
ghm:IrrigationValve rdf:type owl:Class ;
                    rdfs:subClassOf ghm:Valve ;
                    rdfs:comment "An irrigation valve is a switch that steers the amount of water in a zone" ;
                    rdfs:label "Irrigation valve" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/IrrigtationSystem
ghm:IrrigtationSystem rdf:type owl:Class ;
