ghm:hasAl_level rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:float ;
                rdfs:comment "This is the level of aluminium in a sample of water measured in µmol/l" ;
                rdfs:label "has al level" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasAverageLength
ghm:hasAverageLength rdf:type owl:DatatypeProperty ;
                     rdfs:domain ghm:PlantGrowthMeasurement ;
                     rdfs:range xsd:integer ;
                     rdfs:comment "The average length of all the plants in a measurement in centimeters" ;
                     rdfs:label "Average length" .


###  https://www.tno.nl/agrifood/ontology/greenhouse-model/hasAverageNumberOfLeavesPerStem
ghm:hasAverageNumberOfLeavesPerStem rdf:type owl:DatatypeProperty ;
                                    rdfs:domain ghm:PlantGrowthMeasurement ;
                                    rdfs:range xsd:integer ;
